{
  "$schema": "https://raw.githubusercontent.com/efreitasn/wrun/master/wrun.schema.json",
  "delayToKill": 1000,
  "fatalIfErr": false,
  "cmds": [
    {
      "fatalIfErr": true,
      "terms": [
